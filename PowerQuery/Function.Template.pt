let 
    doc = [
        Documentation.Name = "Function Template", 
        Documentation.FunctionName = "Sample.Function",
        Documentation.Description = "A Template that can be used as a starting point to implement your own functions", 
        Documentation.Examples = {
            [
                Description = "...",
                Code = "FunctionName(    )",
                Result = "..."
            ],
            [
                Description = "...",
                Code = "FunctionName(    )",
                Result = "..."
            ]
        }
    ],
    fn = ... //Paste the written function here, leaving "fn =" in place 
in 
    Value.ReplaceType(fn, Value.ReplaceMetadata(Value.Type(fn), doc))
